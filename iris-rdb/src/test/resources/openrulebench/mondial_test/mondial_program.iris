isa(prov(?Y, ?X), 'provi') :- p(?S, ?T, ?X, ?Y).
att(prov(?Y, ?X), 'number', ?A) :- p(?A, ?S, ?X, ?Y).
att(prov(?Y, ?X), 'number2', ?D) :- p(?S, ?D, ?X, ?Y).
p(?A, ?D, ?X, ?Y) :- province(?Y,?X,?D,?T1,?T2,?T3), city(?T4,?X,?Y,?A,?T5,?T6).
queryrule(?X,?A,?D) :- isa(prov(?X,'cn'),'provi'), att(prov(?X,'cn'),'number',?A), att(prov(?X,'cn'),'number2',?D).

?- queryrule(?X,?A,?D).
